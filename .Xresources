!------------!
! Extensions !
!------------!
!URxvt.perl-ext-common:      font-size
!URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
!URxvt.url-select.launcher:  google-chrome
!URxvt.url-select.underline: true
!URxvt.keysym.M-u:           perl:url-select:select_next
!URxvt.keysym.M-Escape:      perl:keyboard-select:activate
!URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt*scrollBar:   false
!URxvt*borderColor: #ffffff


!------------------!
! Change font size !
!------------------!
!URxvt.keysym.C-+:     perl:font-size:incglobal
!URxvt.keysym.C-Down:  perl:font-size:decglobal
!URxvt.keysym.C-S-Up:   perl:font-size:increase
!URxvt.keysym.C-S-Down: perl:font-size:decrease


!-------!
! Fonts !
!-------!
!URxvt*font: xft:Ubuntu Mono:pixelsize=16:antialias=true:hinting=true
!URxvt*boldFont: xft:Ubuntu Mono:bold:pixelsize=16:antialias=true:hinting=true
URxvt*font:             xft:Terminess Powerline:pixelsize=16:antialias=false
URxvt*boldFont:         xft:Terminess Powerline:pixelsize=16:bold:antialias=false
!URxvt.font:     9x15
!URxvt.boldFont: 9x15bold
!URxvt.letterSpace: 0
Xft*dpi: 96


!---------!
! Colours !
!---------!

*background: #242424
*foreground: #e2e2e5

URxvt*depth: 32
URxvt*background: rgba:0000/0000/0000/6666
URxvt*foreground: #ffffff

! Black
*color0:    rgb:00/00/00
*color8:    rgb:00/00/00

! Red
*color1:    rgb:ff/5d/28
*color9:    rgb:ff/5d/28

! Green
*color2:    rgb:b1/d6/31
*color10:   rgb:b1/d6/31

! Yellow
*color3:    rgb:e6/ea/50
*color11:   rgb:e6/ea/50

! Blue
*color4:    rgb:7e/8a/a2
*color12:   rgb:7e/8a/a2

! Magenta
*color5:    rgb:af/5f/5f
*color13:   rgb:af/5f/5f

! Cyan
*color6:    rgb:ac/f0/f2
*color14:   rgb:ac/f0/f2

! White
*color7:    rgb:e2/e2/e5
*color15:   rgb:e2/e2/e5

